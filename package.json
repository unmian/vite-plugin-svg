{
  "name": "@unmian/vite-plugin-svg",
  "version": "0.2.2",
  "description": "Vite plugin for svg sprite",
  "keywords": [
    "svg",
    "sprite",
    "vite",
    "plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unmian/vite-plugin-svg"
  },
  "funding": "https://github.com/sponsors/unmian",
  "author": "Quarter <master@shiruihua.cn>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/unmian/vite-plugin-svg",
  "bugs": "https://github.com/unmian/vite-plugin-svg/issues",
  "scripts": {
    "prepare": "husky install",
    "dev": "vitest dev",
    "lint": "f2elint scan",
    "test": "vitest run",
    "prepublishOnly": "tsup src/index.ts --dts --format cjs,esm",
    "publish:private": "pnpm publish --no-git-checks --registry https://gitea.lescity.com.cn/api/packages/Quarter/npm/",
    "unpublish:private": "pnpm unpublish --force https://gitea.lescity.com.cn/api/packages/Quarter/npm/"
  },
  "peerDependencies": {
    "vite": "3.0.9"
  },
  "devDependencies": {
    "@rollup/pluginutils": "4.2.1",
    "@types/node": "18.7.13",
    "@types/svg-parser": "2.0.3",
    "@types/svg-sprite": "0.0.33",
    "@types/svgo": "2.6.4",
    "eslint": "8.23.0",
    "f2elint": "2.2.1",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "rollup": "2.78.1",
    "tsup": "6.2.3",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vitest": "0.22.1"
  },
  "dependencies": {
    "@unmian/universal-utils-node": "^0.5.0",
    "svg-parser": "^2.0.4",
    "svg-sprite": "^1.5.4",
    "svgo": "^2.8.0"
  }
}
